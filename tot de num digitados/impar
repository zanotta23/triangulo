algoritmo "semnome"
var
   c, v, tot010, simp: inteiro
inicio
   para c <- 1 ate 6 faca
      escreva (" valor ")
      leia (v)
      SE (v >= 0) e (v <= 10) entao
         tot010 <- tot010 + 1
         se ( V%2=1)entao
            simp <- simp + v
         fimse

      fimse
   fimpara
   escreval ("Ao todo foram ", Tot010, " valores entre 0 e 10")
   Escreval ("Nesse intervalo, a soma de impares foi ", simp)
fimalgoritmo
